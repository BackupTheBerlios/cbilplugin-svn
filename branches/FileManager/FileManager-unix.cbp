<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="File Manager Plugin" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libFileManager.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option object_output="$(#cb)/.objs/plugins/contrib/FileManager" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/FileManager.zip manifest.xml Resources/fileexplorersettings.xrc" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="`pkg-config --cflags gnome-vfs-2.0`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/include/wxscintilla/include" />
			<Add directory="$(#cb)/include/wxFlatNotebook/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add option="`pkg-config --libs gnome-vfs-2.0`" />
			<Add library="codeblocks" />
			<Add library="wxscintilla" />
			<Add directory="$(#cb)/devel" />
		</Linker>
		<Unit filename="FileExplorer.cpp" />
		<Unit filename="FileExplorer.h" />
		<Unit filename="FileExplorerSettings.cpp" />
		<Unit filename="FileExplorerSettings.h" />
		<Unit filename="FileExplorerUpdater.cpp" />
		<Unit filename="FileExplorerUpdater.h" />
		<Unit filename="FileManager.cpp" />
		<Unit filename="FileManager.h" />
		<Unit filename="Resources/fileexplorersettings.xrc" />
		<Unit filename="directorymonitor.cpp" />
		<Unit filename="directorymonitor.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
