<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Power Shell Plugin" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)\devel\share\codeblocks\plugins\PowerShell" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#cb)\devel" />
				<Option object_output="$(#cb)\.objs\plugins\contrib\PowerShell" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug -nd" />
				<Option host_application="codeblocks.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(#cb.include)" />
					<Add directory="$(#cb.include)\wxscintilla\include" />
					<Add directory="$(#cb.include)\wxFlatNotebook\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw28$(WX_SUFFIX)" />
					<Add directory="$(#cb)\devel" />
					<Add directory="$(#wx.lib)\gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)\devel\share\codeblocks\PowerShell.zip manifest.xml" />
				</ExtraCommands>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_CFG" value="" />
				</Environment>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="CmdConfigDialog.cpp" />
		<Unit filename="CmdConfigDialog.h" />
		<Unit filename="PipedProcessCtrl.cpp" />
		<Unit filename="PipedProcessCtrl.h" />
		<Unit filename="PowerShell.cpp" />
		<Unit filename="PowerShell.h" />
		<Unit filename="Resources\dialogs.fbp" />
		<Unit filename="ShellCtrlBase.cpp" />
		<Unit filename="ShellCtrlBase.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Unit filename="shellproperties.cpp" />
		<Unit filename="shellproperties.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
