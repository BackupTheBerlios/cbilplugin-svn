<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PyDebugger-Unix" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libPyDebugger.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)/devel/run.sh" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/PyDebugger.zip manifest.xml" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="wx=$wx_LINUX" value="" />
				<Variable name="py" value="python2.5" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add directory="$(#cb)/devel" />
		</Linker>
		<Unit filename="ConfigDialog.cpp" />
		<Unit filename="ConfigDialog.h" />
		<Unit filename="PyDebugger.cpp" />
		<Unit filename="PyDebugger.h" />
		<Unit filename="Resources/dialogs.fbp">
			<Option target="default" />
		</Unit>
		<Unit filename="dialogs.cpp" />
		<Unit filename="dialogs.h" />
		<Unit filename="manifest.xml">
			<Option target="default" />
		</Unit>
		<Unit filename="pdb2.py">
			<Option target="default" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
