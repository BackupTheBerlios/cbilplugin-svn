<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Interpreted Languages Plugin" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libInterpretedLangs.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/InterpretedLangs.zip manifest.xml" />
				</ExtraCommands>
			</Target>
			<Target title="il-library">
				<Option output="$(#cb)/devel/il-common.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="il-plugin">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libInterpretedLangs.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="$(#cb)/devel/il-common.so" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/InterpretedLangs.zip manifest.xml" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/include/wxscintilla/include" />
			<Add directory="$(#cb)/include/wxFlatNotebook/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add directory="$(#cb)/devel" />
		</Linker>
		<Unit filename="ConfigDialog.cpp">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="ConfigDialog.h">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="FileExplorer.cpp">
			<Option target="il-library" />
			<Option target="default" />
		</Unit>
		<Unit filename="FileExplorer.h">
			<Option target="il-library" />
			<Option target="default" />
		</Unit>
		<Unit filename="FileExplorerSettings.cpp">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="FileExplorerSettings.h">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="InterpretedLangs.cpp">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="InterpretedLangs.h">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="Resources/dialogs.fbp" />
		<Unit filename="Resources/il_resources.xrc" />
		<Unit filename="ShellCtrl.cpp">
			<Option target="default" />
			<Option target="il-library" />
		</Unit>
		<Unit filename="ShellCtrl.h">
			<Option target="default" />
			<Option target="il-library" />
		</Unit>
		<Unit filename="il_globals.cpp">
			<Option target="default" />
			<Option target="il-library" />
		</Unit>
		<Unit filename="il_globals.h">
			<Option target="default" />
			<Option target="il-library" />
		</Unit>
		<Unit filename="interpreterproperties.cpp">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="interpreterproperties.h">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Unit filename="manifest.xml">
			<Option target="default" />
			<Option target="il-plugin" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
