<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="InterpretedLangs" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)\devel\share\codeblocks\plugins\InterpretedLangs.dll" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#cb)\devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug" />
				<Option host_application="$(#cb)\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DTIXML_USE_STL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(#cb.include)" />
					<Add directory="$(#cb.include)\wxscintilla\include" />
					<Add directory="$(#cb)\include\wxFlatNotebook\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw26u" />
					<Add directory="$(#cb)\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)\devel\share\codeblocks\InterpretedLangs.zip manifest.xml" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="ConfigDialog.cpp" />
		<Unit filename="ConfigDialog.h" />
		<Unit filename="InterpretedLangs.cpp" />
		<Unit filename="InterpretedLangs.h" />
		<Unit filename="Resources\dialogs.fbp" />
		<Unit filename="ShellCtrl.cpp" />
		<Unit filename="ShellCtrl.h" />
		<Unit filename="interpreterproperties.cpp" />
		<Unit filename="interpreterproperties.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="pyeval.py" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
