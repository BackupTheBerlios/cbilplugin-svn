<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PythonCodeCompletion" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="XMLRPC">
				<Option output="XMLRPC\XmlRpc" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_MBCS" />
					<Add option="-D_LIB" />
					<Add directory="XMLRPC\include" />
				</Compiler>
				<Linker>
					<Add library="Ws2_32" />
				</Linker>
			</Target>
			<Target title="CodeCompletion">
				<Option output="PythonCodeCompletion" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_LIB" />
					<Add directory="XMLRPC\include" />
					<Add directory="$(#cb)\include" />
					<Add directory="$(#cb)\sdk\wxscintilla\include" />
					<Add directory="$(#wx)\include" />
					<Add directory="$(#wx)\lib\gcc_dll\mswu" />
				</Compiler>
				<Linker>
					<Add library="XmlRpc" />
					<Add library="codeblocks" />
					<Add library="wxmsw28u" />
					<Add library="Ws2_32" />
					<Add directory="XMLRPC" />
					<Add directory="$(#wx)\lib\gcc_dll" />
					<Add directory="$(#cb)\devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 PythonCodeCompletion.zip manifest.xml" />
					<Add after="zip -r9 PythonCodeCompletion.cbplugin PythonCodeCompletion.dll PythonCodeCompletion.zip python/python_completion_server.py" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="XMLRPC;CodeCompletion;" />
		</VirtualTargets>
