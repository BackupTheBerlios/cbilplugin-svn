<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PyDebugger-Unix" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="PyDebugger.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug --multiple-instance -ns -ni" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-ansi" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 PyDebugger.zip manifest.xml Resources/debugger_options_dlg.xrc" />
					<Add after="zip -r9 PyDebugger.cbplugin PyDebugger.so PyDebugger.zip" />
				</ExtraCommands>
			</Target>
			<Target title="test">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libPyDebugger.so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#cb)/devel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--personality=debug --multiple-instance -ns -ni" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/PyDebugger.zip manifest.xml" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="py" value="python2.5" />
				<Variable name="wx=$wx_LINUX" value="" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="`pkg-config --cflags codeblocks`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add option="`pkg-config --libs codeblocks`" />
			<Add directory="$(#cb)/devel" />
		</Linker>
		<Unit filename="DebuggerController.h">
			<Option target="default" />
		</Unit>
		<Unit filename="PyDebugger.cpp">
			<Option target="default" />
		</Unit>
		<Unit filename="PyDebugger.h">
			<Option target="default" />
		</Unit>
		<Unit filename="Resources/debugger_options_dlg.xrc" />
		<Unit filename="Resources/dialogs.fbp" />
		<Unit filename="debuggeroptionsdlg.cpp" />
		<Unit filename="debuggeroptionsdlg.h" />
		<Unit filename="dialogs.cpp" />
		<Unit filename="dialogs.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
